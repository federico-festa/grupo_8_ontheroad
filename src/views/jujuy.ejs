    <script type="text/javascript">
        function changeTab(id, id2, id3) {
            var elemento = [document.getElementById(id), document.getElementById(id2), document.getElementById(id3)];
            if (window.getComputedStyle(elemento[0]).display === 'block') {
                elemento[0].style.display = 'block';
                elemento[1].style.display = 'none';
                elemento[2].style.display = 'none';
            } else {
                elemento[0].style.display = 'block';
                elemento[1].style.display = 'none';
                elemento[2].style.display = 'none';
            }
        }
    </script>
